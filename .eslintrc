{
  "env": {
    "node": true
  },
  "rules": {
    "brace-style": [2, "1tbs", { "allowSingleLine": true }],
    "camelcase": 0, // OFF
    "comma-spacing": 2,
    "consistent-return": 0,
    "consistent-this": [ 0, "self" ], // OFF
    "curly": 0, // OFF
    "dot-notation": 2,
    "eol-last": 0,
    "eqeqeq": 0,
    "handle-callback-err": 0,
    "key-spacing": 0,
    "new-cap": 0,
    "new-parens": 2,
    "no-alert": 0,
    "no-array-constructor": 2,
    "no-caller": 2,
    "no-comma-dangle": 0,
    "no-cond-assign": 2,
    "no-constant-condition": 2,
    "no-control-regex": 2,
    "no-debugger": 2,
    "no-delete-var": 2,
    "no-dupe-keys": 2,
    "no-else-return": 2,
    "no-empty": 2,
    "no-empty-class": 2,
    "no-empty-label": 2,
    "no-eval": 2,
    "no-ex-assign": 2,
    "no-extend-native": 2,
    "no-extra-boolean-cast": 2,
    "no-extra-semi": 2,
    "no-extra-strict": 2,
    "no-fallthrough": 2,
    "no-func-assign": 2,
    "no-implied-eval": 2,
    "no-inner-declarations": 2,
    "no-invalid-regexp": 2,
    "no-iterator": 2,
    "no-label-var": 2,
    "no-labels": 2,
    "no-lone-blocks": 2,
    "no-lonely-if": 2,
    "no-loop-func": 2,
    "no-mixed-requires": 2,
    "no-mixed-spaces-and-tabs": 2,
    "no-multi-spaces": 0,
    "no-multi-str": 2,
    "no-native-reassign": 2,
    "no-negated-in-lhs": 0,
    "no-nested-ternary": 2,
    "no-new": 2,
    "no-new-func": 2,
    "no-new-object": 2,
    "no-new-require": 2,
    "no-new-wrappers": 2,
    "no-obj-calls": 2,
    "no-octal": 2,
    "no-octal-escape": 2,
    "no-path-concat": 2,
    "no-process-exit": 0,
    "no-proto": 2,
    "no-redeclare": 2,
    "no-regex-spaces": 2,
    "no-return-assign": 2,
    "no-script-url": 2,
    "no-sequences": 2,
    "no-shadow": 0, // Should be enabled, but isn't trivial to fix
    "no-shadow-restricted-names": 2,
    "no-space-before-semi": 2,
    "no-spaced-func": 2,
    "no-sparse-arrays": 2,
    "no-trailing-spaces": 0,
    "no-undef": 0,
    "no-undef-init": 2,
    "no-underscore-dangle": 0, // OFF
    "no-unreachable": 2,
    "no-unused-expressions": 0,
    "no-unused-vars": [2, {"vars": "all", "args": "none"}],
    "no-use-before-define": 0,
    "no-warning-comments": 0, // 1
    "no-with": 2,
    "no-wrap-func": 2,
    "quotes": [ 0, "single", "avoid-escape" ],
    "semi": 2,
    "space-after-keywords": 2,
    "space-before-blocks": [2, "always"],
    "space-infix-ops": 0,
    "space-return-throw-case": 2,
    "space-unary-ops": 2,
    "strict": 0,
    "use-isnan": 2,
    "valid-typeof": 2,
    "yoda": 2
  }
}
